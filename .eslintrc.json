{
  "plugins": ["prettier", "unicorn"],
  "extends": [
    "plugin:unicorn/recommended",
    "plugin:prettier/recommended",
    "prettier",

    "next/core-web-vitals"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-debugger": "off",
    "no-console": 0
  }
}
